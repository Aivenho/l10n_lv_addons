		    <field name="view_type">form</field>
		    <field name="view_mode">form</field>
            <field name="target">new</field>
		    <field name="view_id" ref="form_view_report_webkit_test"/>
	    </record>

        <menuitem id="menu_report_testing_main" name="Testing" parent="base.menu_reporting" sequence="20" groups="base.group_no_one"/>

	    <menuitem action="view_action_report_webkit_test_menu" id="menu_report_webkit_test" name="Character Test" parent="menu_report_testing_main" groups="base.group_no_one"/>

    </data>
