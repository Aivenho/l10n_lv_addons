<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

	    <record id="form_view_report_webkit_test" model="ir.ui.view">
	    	<field name="name">Report Webkit Test</field>
	    	<field name="model">report.webkit.test</field>
	    	<field name="type">form</field>
	    	<field name="arch" type="xml">
	    		<form string="Report Webkit Test" version="7.0">
