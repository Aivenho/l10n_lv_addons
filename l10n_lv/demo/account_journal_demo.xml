<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!--
        Account Journal
        -->

        <record id="account.sales_journal" model="account.journal">
            <field model="account.account" name="default_credit_account_id" eval="ref('a_sale')"/>
            <field model="account.account" name="default_debit_account_id" eval="ref('a_sale')"/>
        </record>
        <record id="account.refund_sales_journal" model="account.journal">
            <field model="account.account" name="default_credit_account_id" eval="ref('a_sale')"/>
            <field model="account.account" name="default_debit_account_id" eval="ref('a_sale')"/>
        </record>

        <record id="account.expenses_journal" model="account.journal">
            <field model="account.account" name="default_debit_account_id" eval="ref('a_expense')"/>
            <field model="account.account" name="default_credit_account_id" eval="ref('a_expense')"/>
        </record>
        <record id="account.refund_expenses_journal" model="account.journal">
            <field model="account.account" name="default_debit_account_id" eval="ref('a_expense')"/>
            <field model="account.account" name="default_credit_account_id" eval="ref('a_expense')"/>
        </record>

        <record id="account.bank_journal" model="account.journal">
            <field model="account.account" name="default_debit_account_id" eval="ref('bnk')"/>
            <field model="account.account" name="default_credit_account_id" eval="ref('bnk')"/>
        </record>
        <record id="account.check_journal" model="account.journal">
            <field model="account.account" name="default_debit_account_id" eval="ref('cash')"/>
            <field model="account.account" name="default_credit_account_id" eval="ref('cash')"/>
        </record>
        <record id="account.cash_journal" model="account.journal">
            <field name="profit_account_id" model="account.account" eval="ref('rsa')" />
            <field name="loss_account_id" model="account.account" eval="ref('rsa')" />
            <field name="internal_account_id" model="account.account" eval="ref('rsa')" />
            <field model="account.account" name="default_debit_account_id" eval="ref('cash')"/>
            <field model="account.account" name="default_credit_account_id" eval="ref('cash')"/>
        </record>

        <record id="account.opening_journal" model="account.journal">
            <field model="account.account" name="default_debit_account_id" eval="ref('o_income')"/>
            <field model="account.account" name="default_credit_account_id" eval="ref('o_expense')"/>
        </record>

        <record id="account.bank_journal_usd" model="account.journal">
            <field model="account.account" name="default_debit_account_id" eval="ref('usd_bnk')"/>
            <field model="account.account" name="default_credit_account_id" eval="ref('usd_bnk')"/>
        </record>

    </data>
</openerp>