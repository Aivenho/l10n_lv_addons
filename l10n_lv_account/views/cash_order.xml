<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

<template id="cash_order_document">
    <t t-call="report.external_layout">
            <div class="page">
                <div class="oe_structure"/>
                <h2>
                    <span>Kases orderis: </span><span t-field="o.ref"/>
                </h2>
                <div class="oe_structure"/>
            </div>
    </t>
</template>

<template id="cash_order">
    <t t-call="report.html_container">
        <t t-foreach="docs" t-as="o">
            <t t-foreach="o.report_line_ids" t-as="line">
                <t t-raw="translate_doc(line.id, 'account.bank.statement.line', 'partner_id.lang', 'l10n_lv_account.cash_order_document')"/>
            </t>
        </t>
    </t>
</template>

</data>
</openerp>